<!DOCTYPE html>
<html>
  <head>
  	<title>Handrogram</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
	<div class="row">
		<div class="main col-md-offset-3 col-md-6">
			<div class="btn-wrapper">
				<div class="row">
					<p><%=link_to "会員の方はこちら", new_user_session_path, class: "btn btn-info col-xs-10 col-xs-offset-1" %></p>
				</div>
				<div class="row"></div>
				<div class="row">
					<p><%=link_to "新規登録の方はこちら", new_user_registration_path, class: "btn btn-success col-xs-10 col-xs-offset-1" %></p>
				</div>
			</div>
		</div>
	</div>
  </body>
</html>