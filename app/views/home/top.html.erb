<!DOCTYPE html>
<html>
  <head>
  	<title>Handrogram</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
	<div class="row">
		<div class="btn-wrapper">
			<div class="main col-md-offset-3 col-md-6">
				<%= image_tag "rogo_top.png", alt: "top_logo", id: "assets", class: "image" %><br><br><br><br>
			</div>
			<div class="field">
				<span><%=link_to "会員の方はこちら", new_user_session_path, class: "btn btn-warning col-xs-2 col-xs-offset-3" %></span>
				<span><%=link_to "新規登録の方はこちら", new_user_registration_path, class: "btn btn-warning col-xs-2 col-xs-offset-3" %></span><br /><br /><br /><br><br><br><br><br>
			</div>
		</div>
	</div>
  </body>
</html>